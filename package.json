    "scaffold": "./scripts/scaffold-block.mjs",
    "storybook": "start-storybook -p 6006",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:dev": "NODE_OPTIONS=--experimental-vm-modules jest --watchAll ./src",
    "test:src": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-vm-modules jest ./src",
    "type:check": "tsc --noEmit"
